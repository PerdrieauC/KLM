import { Connaissance } from "../Connaissance";
import { Etape } from "./Etape";

export class Procedural extends Connaissance {
    public Etapes: Etape[];

    constructor(nom: string, description: string) {
        super(nom, description);
        this.Etapes = [];
    }

    public addEtape(etape: Etape): void {
        this.Etapes.push(etape);
    }

    public toString(): string {
        return `Procedural{Etapes=${this.Etapes}}`;
    }
}
